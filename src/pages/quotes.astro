<style>
  body {
    background-color: pink;
  }

  main {
    display: flex;
    flex-direction: column;
    padding: 0 2rem;
  }

  div {
    max-width: 50rem;
  }

  img {
    margin: 0.75rem auto 0 auto;
    width: 30rem;
    filter: drop-shadow(0 0 3px aliceblue);
  }

  blockquote {
    margin: 2.5rem 1rem;
    background-color: aliceblue;
    padding: 2rem;
    box-shadow: 1rem 1rem 0 salmon;
  }

  blockquote:first-of-type {
    margin-top: 1.5rem;
  }

  .at b {
    color: blue;
  }
</style>

